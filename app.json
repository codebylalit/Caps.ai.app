{
  "expo": {
    "name": "Hashly",
    "slug": "capsai",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/Hashly.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/HashlyT.png",
      "resizeMode": "contain",
      "backgroundColor": "#74B3AD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caps.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes for payments"
      }
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFF5E1"
      },
      "package": "com.caps.ai",
      "fullscreen": true,
      "permissions": [
        "CAMERA"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "caps",
              "host": "payment",
              "pathPrefix": "/"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "166464ff-5274-416e-ae5d-9b579bbc9c32"
      }
    },
    "owner": "heylalit",
    "scheme": "caps",
    "plugins": []
  }
}