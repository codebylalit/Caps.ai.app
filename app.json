{
  "expo": {
    "name": "Caps.ai",
    "slug": "capsai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF5E1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caps.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes for payments"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFF5E1"
      },
      "package": "com.caps.ai",
      "fullscreen": true,
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "166464ff-5274-416e-ae5d-9b579bbc9c32"
      }
    },
    "owner": "heylalit",
    "scheme": "caps"
  }
}
